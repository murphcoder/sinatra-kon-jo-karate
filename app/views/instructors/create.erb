<h2>Create New Instructor</h2>
<form action="/instructors/new" method="post">
<table>
<tr>
<td><label for="name">Name</label></td>
<td><input type="text" name="instructor[name]" id="name"></td>
</tr>
<tr>
<td><label for="email">Email</label></td>
<td><input type="text" name="instructor[email]" id="email"></td>
</tr>
<tr>
<td><label for="password">Password</label></td>
<td><input type="password" name="instructor[password]" id="password"></td>
</tr>
<tr>
<td><label for="cell_phone">Cell Phone</label></td>
<td><input type="text" name="instructor[cell_phone]" id="cell_phone"></td>
</tr>
<tr>
<td><label for="address">Description</label></td>
<td><textarea id="address" name="instructor[description]" rows="4" cols="50">
</textarea></td>
</tr>
</table>
<input type="checkbox" id="admin" name="instructor[admin?]" value="true">
<label for="admin">Admin?</label><br><br>
<input type="submit" name="create" id="create" value="Create"><br>
</form>